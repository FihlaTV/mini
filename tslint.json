{
	"rules": {
		"class-name": true,
		"comment-format": [true, "check-space"],
		"curly": true,
		"eofline": false,
		"forin": true,
		"indent": [true, "tabs"],
		"no-duplicate-variable": true,
		"no-empty-interface": true,
		"no-eval": true,
		"no-internal-module": true,
		"no-namespace": true,
		"no-reference": true,
		"no-trailing-whitespace": true,
		"no-var-keyword": true,
		"quotemark": [true, "double"],
		"semicolon": true,
		"typedef": true
	}
}
